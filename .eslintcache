ne\\src\\App.js",["63"],"C:\\Users\\arya\\Desktop\\code\\react\\gdrive-clone\\src\\reportWebVitals.js",[],"C:\\Users\\arya\\Desktop\\code\\react\\gdrive-clone\\src\\components\\Home.js",["64"],"import React,{useState} from 'react'\r\nimport Fileview from './Fileview'\r\n\r\nfunction Home() {\r\n    return (\r\n        <div className='home'>\r\n            <Fileview/>\r\n            \r\n        </div>\r\n    )\r\n}\r\n\r\nexport default Home\r\n","C:\\Users\\arya\\Desktop\\code\\react\\gdrive-clone\\src\\components\\Header.js",["65"],"import { Avatar,IconButton } from '@material-ui/core'\r\nimport {ExpandMore, SearchRounded, SettingsOutlined ,HelpOutline} from '@material-ui/icons'\r\nimport AppsIcon from '@material-ui/icons/Apps';\r\nimport React from 'react'\r\nimport './header.css'\r\n\r\nfunction Header() {\r\n    return (\r\n        <div className='Header'>\r\n            <div className='logo-sec'>\r\n                <img src='https://cdn1.iconfinder.com/data/icons/logotypes/32/google-drive-512.png'/>\r\n                <span>Drive</span>\r\n            </div>\r\n            <div className='search-sec'>\r\n               <IconButton> <SearchRounded/></IconButton>\r\n                <input type='text' placeholder='Search in Drive'/>\r\n                <IconButton><ExpandMore/></IconButton>\r\n            </div>\r\n            <div className='other-sec'>\r\n            <IconButton> <HelpOutline/></IconButton>\r\n            <IconButton><SettingsOutlined/></IconButton>\r\n            <IconButton> <AppsIcon/></IconButton>\r\n                <Avatar/>\r\n            </div>\r\n\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default Header\r\n","C:\\Users\\arya\\Desktop\\code\\react\\gdrive-clone\\src\\components\\SideBar.js",["66","67"],"import { CloudQueueOutlined, DeleteForever, DeleteOutline, InsertDriveFile, QueryBuilder, SignalCellularNull, StarBorder } from '@material-ui/icons'\r\nimport React from 'react'\r\nimport SidebarOption from './SidebarOption'\r\nimport GroupIcon from '@material-ui/icons/Group';\r\nimport './main.css';\r\nimport AddNewFile from './AddNewFile';\r\nfunction SideBar() {\r\n    return (\r\n        <div className='sidebar'>\r\n            <AddNewFile/>\r\n            <SidebarOption icon={<InsertDriveFile/>} text='My Drive'/>\r\n            <SidebarOption icon={<GroupIcon/>} text='Shared with me'/>\r\n            <SidebarOption icon={<QueryBuilder/>} text='Recents'/>\r\n            <SidebarOption icon={<StarBorder/>} text='Starred'/>\r\n            <SidebarOption icon={<DeleteOutline/>} text='Trash'/>\r\n            <br/>\r\n            <hr/>\r\n            <SidebarOption icon={<CloudQueueOutlined/>} text='Storage'/>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default SideBar\r\n","C:\\Users\\arya\\Desktop\\code\\react\\gdrive-clone\\src\\components\\Sideicons.js",[],"C:\\Users\\arya\\Desktop\\code\\react\\gdrive-clone\\src\\components\\SidebarOption.js",[],"C:\\Users\\arya\\Desktop\\code\\react\\gdrive-clone\\src\\components\\AddNewFile.js",["68"],"import React,{useState} from 'react'\r\nimport './main.css'\r\nimport AddIcon from '@material-ui/icons/Add';\r\nimport Modal from 'react-modal'\r\nimport { Button } from '@material-ui/core';\r\nimport auth,{db,storage} from '../firebasefile'\r\nimport firebase from 'firebase'\r\n\r\nModal.setAppElement('#root');\r\nexport default function AddNewFile() {\r\n    const [open, setopen] = useState(false);\r\n    const [file, setfile] = useState(null);\r\n    const [isupload,setuploading] = useState(false);\r\n    const openHandle=()=> {setopen(true)}\r\n    const closeHandle=()=> {setopen(false)}\r\n    const filehandler=(e)=> {\r\n       if(e.target.files[0]){\r\n            setfile(e.target.files[0]);\r\n          \r\n        }\r\n    }\r\n    const uploadHandle=()=> {\r\n        if(file){\r\n            setuploading(true);\r\n        storage.ref(`files/${file.name}`).put(file).then(snapshot=>{\r\n            console.log(snapshot);\r\n            storage.ref('files').child(file.name).getDownloadURL().then(url=>{\r\n                db.collection('myfiles').add({\r\n                    timestamp\r\n                    :firebase.firestore.FieldValue.serverTimestamp(),\r\n                    url:url,\r\n                    name:file.name,\r\n                    size:snapshot._delegate.bytesTransferred,\r\n                });\r\n                setuploading(false);\r\n                setopen(false);\r\n                \r\n            })\r\n        })\r\n        }\r\n\r\n    }\r\n    \r\n    return (\r\n        <div>\r\n            <div className='new-button' onClick={openHandle}>\r\n                <AddIcon/><h3>New</h3>\r\n            </div>\r\n            <Modal\r\n            isOpen={open}\r\n            shouldCloseOnOverlayClick={false}\r\n            style={{\r\n                overlay:{\r\n                    backgroundColor:'grey',\r\n    \r\n                },\r\n                content:{\r\n                    width:'400px',\r\n                    position:'absolute',\r\n                    border:'2px solid #000',\r\n                    top:'50%',\r\n                    left:'50%',\r\n                    transform:'translate(-50%,-50%)',\r\n                    height:'200px'\r\n                }\r\n            }}\r\n            >\r\n                <p>select file to upload.</p>\r\n              {  isupload ? \r\n                <h3>Uploading.....</h3>:<div className='modalupload'>\r\n                <input type='file' onChange={(e)=>filehandler(e)}/>\r\n                <br/>\r\n                <Button  onClick={()=>closeHandle()}>close</Button>\r\n                <Button onClick={()=>uploadHandle()}>upload</Button>\r\n                </div>}\r\n          \r\n            </Modal>\r\n\r\n        </div>\r\n    )\r\n}\r\n","C:\\Users\\arya\\Desktop\\code\\react\\gdrive-clone\\src\\firebasefile.js",["69"],"C:\\Users\\arya\\Desktop\\code\\react\\gdrive-clone\\src\\components\\Fileview.js",["70","71"],"import React,{useEffe